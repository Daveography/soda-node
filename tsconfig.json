{
  "extends": "ts-node/node16/tsconfig.json",
  "compileOnSave": true,
  "compilerOptions": {
    "baseUrl": "src",
    "outDir": "dist",
    "sourceMap": false,
    "declaration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": [],
    "lib": [
      "ES2019"
    ],
    "plugins": [
      {
        "transform": "ts-auto-mock/transformer"
      }
    ]
  },
  "include": [
    "src"
  ]
}