{
  "name": "soda-node",
  "description": "Socrata SODA client for Node",
  "version": "0.4.0",
  "author": {
    "name": "Dave Sutherland",
    "email": "dave@daveography.ca",
    "url": "https://github.com/Daveography"
  },
  "homepage": "https://github.com/Daveography/soda-node",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Daveography/soda-node.git"
  },
  "keywords": [
    "socrata",
    "soda",
    "soql",
    "node",
    "open data"
  ],
  "scripts": {
    "clean": "rd /q /s out",
    "build": "tsc",
    "test": "ts-node --project spec/tsconfig.json node_modules/jasmine/bin/jasmine",
    "lint": "eslint .",
    "prepare": "npm run clean && npm run build && npm test && npm run lint",
    "prepublishOnly": "echo 'Do not run npm publish directly, use npm run dist' && exit 1",
    "preDist": "npm run prepare && rd /q /s dist",
    "distCopy": "cp out/src/** dist && cp \"{README.md,LICENSE}\" dist && cp package.json dist",
    "dist": "npm run preDist && npm run distCopy && cd dist && npm publish --ignore-scripts"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "dependencies": {
    "axios": "^1.3.2",
    "axios-observable": "^2.0.0",
    "geojson": "^0.5.0",
    "reflect-metadata": ">=0.1",
    "rxjs": ">=7",
    "terraformer-wkt-parser": ">=1.2"
  },
  "devDependencies": {
    "@types/geojson": "^7946.0.10",
    "@types/jasmine": ">=4.3.1",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": ">=5.51.0",
    "@typescript-eslint/parser": ">=5.51.0",
    "eslint": ">=8.33.0",
    "eslint-config-prettier": ">=8.6.0",
    "eslint-plugin-jsdoc": ">=39.8.0",
    "eslint-plugin-prefer-arrow": "^1.2.1",
    "jasmine": ">=4.5.0",
    "jasmine-core": ">=4.5.0",
    "npm": ">=6.11.3",
    "prettier-eslint": ">=15.0.1",
    "ts-auto-mock": ">=3.6.4",
    "ts-node": ">=10.9.1",
    "ts-patch": "^2.1.0",
    "tslib": ">=2.5.0",
    "typescript": ">=4.9.5"
  }
}
